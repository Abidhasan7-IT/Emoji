    "import emoji\n",
    "\n",
    "def  text_toemoji(text):\n",
    "    return emoji.emojize(text)\n",
    "\n",
    "input_text= input(\"Enter text For Emoji: \")\n",
    "\n",
    "Emoji_convert= text_toemoji(input_text)\n",
    "\n",
    "print(\"Converted to Emoji: \"+ Emoji_convert)\n"
